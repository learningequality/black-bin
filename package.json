{
  "name": "black-fmt",
  "version": "0.0.1",
  "description": "Wrapper around binaries of the Black Python auto-formatter",
  "bin": {
    "black-fmt": "./cli.js"
  },
  "scripts": {
    "dev-prettier-watch": "chokidar '**/*.js' -c \"test '{event}' = 'change' && prettier --write '{path}'\""
  },
  "engines": {
    "node": ">=0.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/learningequality/black-fmt.git"
  },
  "keywords": [
    "black",
    "python",
    "wrapper"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/learningequality/black-fmt/issues"
  },
  "homepage": "https://github.com/learningequality/black-fmt",
  "dependencies": {
    "chokidar": "^2.1.5",
    "commander": "^2.19.0",
    "glob": "^7.1.3",
    "npmlog": "^4.1.2",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.2",
    "prettier": "^1.16.4"
  }
}
